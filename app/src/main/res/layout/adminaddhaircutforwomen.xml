            android:layout_marginTop="397dp"
            android:layout_marginEnd="144dp"
            android:layout_marginBottom="216dp"
            android:text="Long Type"
            android:textAlignment="center"
            android:textColor="@color/white"
            android:textSize="16dp">


        </TextView>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="19dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"


            android:layout_alignParentBottom="true"
            android:layout_marginStart="139dp"
            android:layout_marginTop="206dp"
            android:layout_marginEnd="133dp"
            android:layout_marginBottom="408dp"
            android:text="Medium Type"
            android:textAlignment="center"
            android:textColor="@color/white"
            android:textSize="16dp">


        </TextView>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="19dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"


            android:layout_alignParentBottom="true"
            android:layout_marginStart="133dp"
            android:layout_marginTop="14dp"
            android:layout_marginEnd="139dp"
            android:layout_marginBottom="600dp"
            android:text="Short Type"
            android:textAlignment="center"
            android:textColor="@color/white"
            android:textSize="16dp">


        </TextView>

        <Button
            android:id="@+id/buttonformen"
            android:layout_width="136dp"
            android:layout_height="34dp"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginEnd="8dp"
            android:layout_marginBottom="3dp"
            android:background="@drawable/buttonlogin"
            android:text="For Men">
        </Button>


    </RelativeLayout>






    <LinearLayout
        android:id="@+id/navigation_bar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#D9D9D9"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent">



        <RelativeLayout
            android:id="@+id/home_layout"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_weight="1">

            <ImageButton
                android:id="@+id/homeadd"
                android:layout_width="59dp"
                android:layout_height="45dp"
                android:layout_alignParentStart="true"
                android:layout_alignParentEnd="true"
                android:layout_marginStart="24dp"
                android:layout_marginEnd="20dp"
                android:scaleType="fitXY"
                android:background="@drawable/homelogo" />


        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/feedback_layout"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_weight="1">

            <ImageButton
                android:id="@+id/feedbackadd"
                android:layout_width="116dp"
                android:layout_height="44dp"
                android:layout_alignParentStart="true"
                android:layout_alignParentEnd="true"
                android:layout_marginStart="11dp"
                android:layout_marginEnd="10dp"
                android:background="@drawable/navigationfeedback"
                android:scaleType="fitXY" />



        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/profile_layout"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_weight="1">

            <ImageButton
                android:id="@+id/profileadd"
                android:layout_width="55sp"
                android:layout_height="48dp"
                android:layout_alignParentStart="true"
                android:layout_alignParentEnd="true"
                android:layout_marginStart="31dp"
                android:layout_marginEnd="27dp"
                android:scaleType="fitXY"
                android:background="@drawable/profilelogo" />


        </RelativeLayout>

    </LinearLayout>


</androidx.constraintlayout.widget.ConstraintLayout>