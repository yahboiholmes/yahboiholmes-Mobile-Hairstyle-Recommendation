            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.576">

            <ImageButton
                android:id="@+id/mediumtypeadd"
                android:layout_width="60dp"
                android:layout_height="55dp"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:layout_alignParentBottom="true"
                android:layout_marginStart="302dp"
                android:layout_marginTop="102dp"
                android:layout_marginEnd="11dp"
                android:layout_marginBottom="7dp"
                android:scaleType="fitXY"
                app:srcCompat="@drawable/photoadd">
            </ImageButton>

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/frameinnnerwomen2"
            android:layout_width="373dp"
            android:layout_height="164dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_gravity="center"
            android:layout_marginStart="6dp"
            android:layout_marginTop="422dp"
            android:layout_marginEnd="7dp"
            android:layout_marginBottom="47dp"
            android:background="@drawable/frameinnerfemale"
            android:clipToOutline="true"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.576">


            <ImageButton
                android:id="@+id/longtypeadd"
                android:layout_width="60dp"
                android:layout_height="55dp"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:layout_alignParentBottom="true"
                android:layout_marginStart="302dp"
                android:layout_marginTop="102dp"
                android:layout_marginEnd="11dp"
                android:layout_marginBottom="7dp"
                android:scaleType="fitXY"
                app:srcCompat="@drawable/photoadd">
            </ImageButton>
        </RelativeLayout>


        <TextView
            android:layout_width="88dp"
            android:layout_height="20dp"
            android:layout_alignParentStart="true"
