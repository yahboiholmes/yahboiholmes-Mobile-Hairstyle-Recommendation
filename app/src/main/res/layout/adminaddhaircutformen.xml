<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#443C54"
    android:gravity="center">


    <RelativeLayout
        android:id="@+id/frameouterwomen"
        android:layout_width="386dp"
        android:layout_height="633dp"
        android:layout_gravity="center"
        android:background="@drawable/frameouterfemale"
        android:clipToOutline="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.444"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.164">

        <RelativeLayout
            android:id="@+id/frameinnnerwomen"
            android:layout_width="372dp"
            android:layout_height="237dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_gravity="center"
            android:layout_marginStart="7dp"
            android:layout_marginTop="37dp"
            android:layout_marginEnd="7dp"
            android:layout_marginBottom="359dp"
            android:background="@drawable/frameinnerfemale"
            android:clipToOutline="true"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.576">

            <ImageButton
                android:id="@+id/addshortype"
                android:layout_width="76dp"
                android:layout_height="52dp"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:layout_alignParentBottom="true"
                android:layout_marginStart="281dp"
                android:layout_marginTop="171dp"
                android:layout_marginEnd="15dp"
                android:layout_marginBottom="14dp"
                android:scaleType="fitXY"
                app:srcCompat="@drawable/photoadd">
            </ImageButton>

        </RelativeLayout>


        <RelativeLayout
            android:id="@+id/frameinnnerwomen2"
            android:layout_width="369dp"
            android:layout_height="254dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_gravity="center"
            android:layout_marginStart="8dp"
            android:layout_marginTop="322dp"
            android:layout_marginEnd="9dp"
            android:layout_marginBottom="57dp"
            android:background="@drawable/frameinnerfemale"
            android:clipToOutline="true"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.576">

            <ImageButton
                android:layout_width="78dp"
                android:layout_height="55dp"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:layout_alignParentBottom="true"
                android:layout_marginStart="281dp"
                android:layout_marginTop="185dp"
                android:layout_marginEnd="10dp"
                android:layout_marginBottom="14dp"
                android:scaleType="fitXY"
                app:srcCompat="@drawable/photoadd">


            </ImageButton>

        </RelativeLayout>

        <TextView
            android:layout_width="88dp"
            android:layout_height="20dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"


            android:layout_alignParentBottom="true"
            android:layout_marginStart="146dp"
            android:layout_marginTop="296dp"
            android:layout_marginEnd="152dp"
            android:layout_marginBottom="317dp"
            android:text="Long Type"
            android:textAlignment="center"
            android:textColor="@color/white"
            android:textSize="16dp">


        </TextView>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="19dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"


            android:layout_alignParentBottom="true"
            android:layout_marginStart="133dp"
            android:layout_marginTop="14dp"
            android:layout_marginEnd="139dp"
            android:layout_marginBottom="600dp"
            android:text="Short Type"
            android:textAlignment="center"
            android:textColor="@color/white"
            android:textSize="16dp">


        </TextView>

        <Button
            android:id="@+id/buttonformen"
            android:layout_width="151dp"
            android:layout_height="38dp"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginEnd="227dp"
            android:layout_marginBottom="7dp"
            android:background="@drawable/buttonlogin"
            android:text="For Women">


        </Button>


    </RelativeLayout>

    <LinearLayout
        android:id="@+id/navigation_bar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#D9D9D9"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent">



        <RelativeLayout
            android:id="@+id/home_layout"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_weight="1">

            <ImageButton
                android:id="@+id/homeadd"
                android:layout_width="59dp"
                android:layout_height="45dp"
                android:layout_alignParentStart="true"
                android:layout_alignParentEnd="true"
                android:layout_marginStart="24dp"
                android:layout_marginEnd="20dp"
                android:scaleType="fitXY"
                android:background="@drawable/homelogo" />


        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/feedback_layout"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_weight="1">

            <ImageButton
                android:id="@+id/feedbackadd"
                android:layout_width="116dp"
                android:layout_height="44dp"
                android:layout_alignParentStart="true"
                android:layout_alignParentEnd="true"
                android:layout_marginStart="11dp"
                android:layout_marginEnd="10dp"
                android:background="@drawable/navigationfeedback"
                android:scaleType="fitXY" />



        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/profile_layout"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_weight="1">

            <ImageButton
                android:id="@+id/profileadd"
                android:layout_width="55sp"
                android:layout_height="48dp"
                android:layout_alignParentStart="true"
                android:layout_alignParentEnd="true"
                android:layout_marginStart="31dp"
                android:layout_marginEnd="27dp"
                android:scaleType="fitXY"
                android:background="@drawable/profilelogo" />


        </RelativeLayout>

    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>